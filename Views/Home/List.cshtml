<div>
    <form action="/Home/AddEmp" >
        <button type="submit" id="btn" class="btn btn-info" onclick="AddEmp">Insert Emp</button>
    </form>
</div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Salary</th>
        </tr>
    </thead>
    <tbody>
        @{
        @foreach(var e in @ViewBag.List){
        <tr>
            <td>@e.Id</td>
            <td>@e.Name</td>
            <td>@e.Salary</td>
        </tr>}}
    </tbody>
</table>

<form action="/Home/UpdateEmp" method="get">
    <input type="text" id="empid" name="empid" />
    <br />
    <br />
    <button type="submit" id="btn">Update Emp</button>
    <button type="submit" id="btn1" formaction="/Home/DeleteEmp" formmethod="post">Delete</button>
</form>